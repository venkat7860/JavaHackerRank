import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int i = 1;
        while(input.hasNext()){
            String sc = input.nextLine();
            System.out.println(i+" " +sc);
            i++;
        }
    }
}
