package InterviewPreparation;

public class SwitchCase {

	public static void main(String[] args) 
	{
		int a=5;
		switch(a)
		{
		case 1:
		{
			System.out.println("A");
			break;

		}
		case 2:
		{
			System.out.println("B");
			break;


		}
		case 3:
		{
			System.out.println("c");
			break;


		}
		case 4:
		{
			System.out.println("d");
			break;


		}
		case 5:
		{
			System.out.println("e");
			break;


		}
		}
	}
}
